{
  "name": "socorro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "npm run build:client && npm run build:backend",
    "build:client": "react-scripts build",
    "build:server": "webpack --config webpack.config.server.js",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "npm run format && npm run lint && npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "start": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,application\" \"npm run start:server:webpack\" \"npm run start:server:nodemon\" \"npm run start:client\"",
    "start:client": "wait-on -l tcp:9000 && react-scripts start",
    "start:client:dev": "react-scripts start",
    "start:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"npm run start:server:webpack\" \"npm run start:server:nodemon\"",
    "start:server:nodemon": "rm -f build/server.js && wait-on -l build/server.js && nodemon build/server.js",
    "start:server:webpack": "webpack --config webpack.config.server.js --watch",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18next": "^19.8.3",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "mongoose": "^5.10.14",
    "node-sass": "^4.0.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-leaflet": "^3.0.4",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/leaflet": "^1.5.19",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^12.19.8",
    "@types/node-sass": "^4.11.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.16.0",
    "concurrently": "^5.3.0",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv-webpack": "^5.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.11",
    "wait-on": "^5.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
